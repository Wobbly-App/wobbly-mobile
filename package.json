{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "typecheck": "yarn tsc --noEmit",
    "pretty": "prettier --check \"src/**/*.{ts,tsx,js,jsx}\"",
    "pretty:fix": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\"",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./",
    "lint:fix": "yarn lint --fix",
    "ci": "yarn typecheck && yarn pretty && yarn lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-merge": "yarn install"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "yarn pretty",
      "yarn lint:fix",
      "git add"
    ]
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.7.1",
    "@react-native-community/masked-view": "^0.1.6",
    "@react-navigation/drawer": "^5.0.0-alpha.41",
    "@react-navigation/native": "^5.0.0-alpha.27",
    "@react-navigation/stack": "^5.0.0-alpha.61",
    "@reduxjs/toolkit": "^1.2.1",
    "@xmpp/client": "^0.9.2",
    "@xmpp/debug": "^0.9.2",
    "expo": "~36.0.0",
    "expo-constants": "~8.0.0",
    "expo-secure-store": "~8.0.0",
    "react": "~16.9.0",
    "react-dom": "~16.12.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-36.0.0.tar.gz",
    "react-native-gesture-handler": "~1.5.0",
    "react-native-gifted-chat": "^0.13.0",
    "react-native-reanimated": "~1.4.0",
    "react-native-safe-area-context": "0.6.0",
    "react-native-screens": "2.0.0-alpha.12",
    "react-native-web": "~0.11.7",
    "react-redux": "^7.1.3",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "thunk": "^0.0.1",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@types/node": "^13.1.7",
    "@types/react": "^16.9.17",
    "@types/react-native": "^0.60.30",
    "@types/react-redux": "^7.1.5",
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/parser": "^2.16.0",
    "babel-preset-expo": "~8.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "husky": "^4.0.10",
    "lint-staged": "^9.5.0",
    "prettier": "1.19.1",
    "typescript": "^3.7.4"
  },
  "private": true
}
